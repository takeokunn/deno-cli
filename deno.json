{
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@^1.0.0-rc.7"
  },
  "tasks": {
    "start": "deno run --allow-read main.ts",
    "dev": "deno run --watch --allow-read main.ts",
    "test": "deno test --allow-read"
  },
  "fmt": {
    "include": ["**/*.ts"],
    "indentWidth": 2,
    "singleQuote": true
  }
}
